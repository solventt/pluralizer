{
    "name": "solventt/pluralizer",
    "description": "Converts a noun from singular to plural",
    "type": "library",
    "keywords": ["pluralizer","noun","singular","plural"],
    "license": "BSD-3-Clause",
    "authors": [{"name": "Chibizov Gr."}],

    "require": {
        "php": "^7.4 || ^8.0"
    },

    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.22"
    },

    "autoload": {
        "psr-4": {
            "Solventt\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Solventt\\Tests\\": "tests"
        }
    },

    "config": {
        "sort-packages": true
    }
}
